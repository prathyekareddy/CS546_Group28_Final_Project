<p>{{error}}</p>
<form method="POST" action="/register" id="registration-form">

    <label for="emailIdInput">Enter email-Id</label><br>
    <input type="text" id="emailIdInput" name="emailIdInput"><br><br>
    <label for="passwordInput">Enter password</label><br>
    <input type="password" id="passwordInput" name="passwordInput"><br><br>

    <label for="firstNameInput">Enter First Name</label><br>
    <input type="text" id="firstNameInput" name="firstNameInput"><br><br>
    <label for="lastNameInput">Enter Last Name</label><br>
    <input type="text" id="lastNameInput" name="lastNameInput"><br><br>

    <label for="genderId">Gender:</label><br>
    <select name="genderInput" id="genderId">
    <option value="Male">Male</option>
    <option value="Female">Female</option>
    <option value="Other">Other</option>
    </select><br><br>

    <label for="userDescriptionInput">Enter userDescription</label><br>
    <input type="text" id="userDescriptionInput" name="userDescriptionInput"><br><br>

    <label for="profileImgUrlInput">Enter profileImgUrl</label><br>
    <input type="text" id="profileImgUrlInput" name="profileImgUrlInput"><br><br>
    <label for="cityInput">Enter city</label><br>
    <input type="text" id="cityInput" name="cityInput"><br><br>

    <label for="stateInput">Enter state</label><br>
    <input type="text" id="stateInput" name="stateInput"><br><br>
    <label for="streetAddressInput">Enter streetAddress</label><br>
    <input type="text" id="streetAddressInput" name="streetAddressInput"><br><br>

    <label for="phoneNumberInput">Enter phoneNumber</label><br>
    <input type="text" id="phoneNumberInput" name="phoneNumberInput"><br><br>
    
    <button type="submit">submit</button>

</form>
<a href="/login">Already have an account? Click here to log-in</a>
<script src='../public/js/register.js'></script>